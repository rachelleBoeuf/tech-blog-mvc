<div class="col-sm-12">
  {{#each data}}
    <div class="project-container row" data-id="{{id}}">
      <div class="post-wrapper row">
        <div class="post-header col-sm-12">
            <div class="row">
              <div class="project-name col-sm-7">{{name}}</div>
              <div class="post-creator col-sm-5">
                <div class="post-date inline-block">{{date_created}}</div>
                <div class="project-user inline-block"> - {{user.name}}</div>
              </div>
            </div>
        </div>
        <div class="post-content col-sm-12">
          {{description}}
        </div>
      </div>
    </div>
  {{/each}}
</div>
